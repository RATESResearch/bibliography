@thesis{deshpande_sensitivity_2003,
	location = {Kingsville},
	title = {Sensitivity and Uncertainty Analysis with Multimedia Models},
	rights = {All rights reserved},
	url = {http://proquest.umi.com/pqdweb?did=766565731&sid=11&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The objective of this study was to assess feasibility and utility of different uncertainty and sensitivity analysis techniques in multimedia environmental applications. Sensitivity analysis techniques considered here include Plackett-Burman technique, response-surface methodology, and first order analysis. Uncertainty techniques include Monte-carlo analysis and point estimate method. These techniques were used to propagate uncertainty in these models assuming ±10\% (local scale) and ±50\% (global scale) uncertainty and variability in the inputs. The analysis was carried out for chlorobenzene and Linear Alkylbenzene Sulfonate ({LAS}) two chemicals exhibiting volatile and non-volatile tendencies respectively. The results indicate that for the conditions employed in this study, all the sensitivity techniques highlighted similar sensitive parameters. Plackett-Burman method is found to be fairly simple tool to identify key inputs, however, it does not account for interaction effects. Fractional factorial method provides this information but it is more complex and time-consuming. Interaction effects can be captured using response-surface method, which is comparatively less complex and hence it may be more preferable in risk-based decision-making. Point estimate method and Monte-Carlo analysis quantify the uncertainty in outputs and give almost similar results. Uncertainty in interactions of inputs is high on global scale for both chemicals, as demonstrated by Monte-Carlo analysis. Monte-Carlo analysis can be applied in combination with first order method to quantify uncertainty in outputs. This information could be useful in risk-based decision-making as the variations in the parameter interactions are significant.},
	institution = {Texas A\&M University - Kingsville},
	type = {phdthesis},
	author = {Deshpande, Bela},
	date = {2003-05},
	annotation = {Volume: Master of Science},
}

