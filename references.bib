@inproceedings{ernest_critical_2008,
	location = {Pittsburgh, {PA}},
	title = {A Critical Review of the Engineering Service Business Models Employed in Public Works Projects},
	abstract = {Open Source ideals and outcomes are eliciting greater and greater prominence in the software engineering sector. An argument can be made that the basic precepts espoused by the Free Software Foundation should be translatable to all branches of engineering, and in particular to projects directly benefiting the public. The concepts of open source and commercial viability are not mutually exclusive, as evidenced by the so called Red Hat Business Model that has seen much press of late. Of key value in the open source business model is the emphasis on the valuation of effort rather than the restriction of knowledge as the basis of market competitiveness. Further advantages include the more rapid leveraging of new knowledge into better products and services, and an overall lowering of the total cost of ownership or commensurate cost of services to the end user. This paper will provide an in-depth review of the business models employed by Civil/Environmental engineers in both the public and the private sector against a backdrop of public works projects. Included will be an assessment of the increasing role that intellectual property protection plays in market competitiveness of engineering service providers, the effect of proprietary technologies have in shaping the demographics of providers, the role that engineering education has in defining this evolution, and the impact this may have on public infrastructure.},
	booktitle = {2008 {ASEE} Annual Conference \& Exposition},
	publisher = {American Society for Engineering Education},
	author = {Ernest, Andrew N. S.},
	date = {2008-06-22},
}

