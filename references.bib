@thesis{bosquez_red_1995-3,
	location = {Kingsville},
	title = {Red Mud Reclamation Using Sewage Sludge Amendments and Bermudagrass},
	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=743294441&sid=1&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {Use of sewage sludge on land is effective for improving soil structure and enhancing vegetation. The addition of sewage sludge provides organic matter and nutrients for growth of Bermudagrass. The study was performed to identify optimal sewage sludge application rates. A series of 6 experiments was designed to evaluate three rates of wet sewage sludge application. The experimental period was 5 months. Soil {pH} values decreased significantly in sludge amended columns. The highest rate of sludge application decreased to a {pH} of 7.3, whereas unamended columns had a {pH} of 8.8. Bermudagrass proliferated in sludge amended columns. Vegetation growth was greater in the highest rate of sludge application, but the lowest rate produced more root biomass. The lowest rate of sludge amended columns also maintained a low hydraulic conductivity (below 0.10 m/s) throughout the experimental period. The amended columns with no grass had a hydraulic conductivity as low as 0.03 m/s. The experiment provides a basis for future field-scale application of sewage sludge.},
	pagetotal = {195},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Bosquez, Juan, Jr.},
	date = {1995-08},
}

@thesis{bosquez_enhanced_1997-3,
	location = {Kingsville},
	title = {Enhanced Biodegradation of Oil in Wetland Environments},
	url = {http://proquest.umi.com/pqdweb?did=740445671&sid=2&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The potential use of nutrients to stimulate in-situ hydrocarbon degradation in intertidal wetland environments was investigated. Previous studies have indicated that biodegradation of hydrocarbons can be stimulated with the addition of nutrients. Ammonium phosphate’s capacity to enhance biodegradation and its potential in the field was studied. Microcosm studies were conducted, and weathered Arabian light crude oil was the hydrocarbon used in the experiments. After 105 days of study, soils with added ammonium phosphate showed a greater degradation than the control soils with no added nutrients. Residual oil showed depletion of n-C17 and n-C18 (relative to hopane) in the nutrient series to be 1.3 times greater than that of the control series. Determination of first order depletion constants, k, showed treatment rate constants significantly larger than the rate constants for the control series.},
	pagetotal = {84},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Bosquez, Armando},
	date = {1997-05},
}

