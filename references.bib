@thesis{swaroop_water_1996,
	location = {Kingsville},
	title = {Water Quality Analysis and Sediment Characterization for the Baffin - Alazan Bay Aquatic System},
	url = {http://proquest.umi.com/pqdweb?did=740286421&sid=2&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {Laguna Madre and adjacent bays and estuaries have experienced a dense algal bloom referred to as the "brown tide" since early 1990. High nutrient levels, contaminants in the system, hypersalinities \& extreme climatic conditions are a few possible causes for brown tide, hypothesized by the researchers. This research study was aimed at creating an extensive database for the Baffin-Alazan Bay aquatic system for water and sediment quality parameters and their correlation with brown tide persistence. Laguna Salada was observed to have the maximum visible effect of the bloom during the research study. High biomass levels (up to \$1.88{\textbackslash}times10{\textbackslash}sp7\$/ml) were recorded. The sediment phosphorus levels were observed to be much higher than sediment nitrate levels. The biomass levels were found to show a negative correlation with water nitrate levels (p \${\textless}\$ 0.05) in the area with maximum visible brown tide effect. The sediment metal concentrations are below those which may cause any significant adverse biological impact.},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Swaroop, Shakti},
	date = {1996-08},
}

@thesis{subramani_validation_1996,
	location = {Kingsville},
	title = {Validation and Evaluation of Intertidal Wetland Microcosms for Bioremediation Research},
	url = {http://proquest.umi.com/pqdweb?did=740285031&sid=2&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The primary goal of this research was to design, construct and validate 36 microcosms to emulate intertidal conditions of marsh wetlands. The experiment was designed to have three treatments of 12 microcosms each. Acclimation is defined as the physiological of behavioral adaptation of organisms to one or more environmental conditions associated with the test method (40CFR. 797.1600.91). The microcosms were constructed and a regular semidiurnal tidal cycle was simulated. Gravity outflow rates, concentrations of nitrates, phosphates, total Kjeldahl nitrogen, and biomass were monitored in both the gravity outflow and reservoir water. Redox of the core was monitored at five different depths. The variation in all the parameters narrowed down to statistically tolerable limits across the three treatments during a two month acclimation period. The microcosm system was validated using statistical methods for an oil spill and testing of bioremediation agents.},
	pagetotal = {75},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Subramani, Rajasugumar},
	date = {1996-08},
}

