@thesis{hernandez_risk_2007,
	location = {Kingsville},
	title = {Risk Based Methodologies for Watershed Management - Integrated Simulation and Optimization Techniques},
	rights = {All rights reserved},
	url = {http://proquest.umi.com/pqdweb?did=1390313031&sid=7&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {In-stream water quality is an important component of watershed management as it defines the intended use. Participatory watershed management involving multiple stakeholders is gaining acceptance for developing waste load allocations ({WLA}). {WLAs} specify the maximum pollutant loadings from permitted compliance systems and are a function of both science and policy. A goal-programming based simulation-optimization approach was developed to assess regional-scale assimilative capacity of a stream. This approach accounts for the heterogeneous nature of the water body regarding flow and non-point source loadings and identifies those sub-regions that are critically stressed. The model was used to develop an optimal loading scheme for the Arroyo Colorado river watershed. A Monte-Carlo reliability analysis was carried out to assess the risks associated with choosing the optimal loading scheme under parametric uncertainty. Ten different models were developed to assess the reliability of the optimal loading scheme under structural uncertainty. The results indicated that San Benito consistently had the highest probability of exceeding the biochemical oxygen demand ({BOD}) standard; similarly, {McAllen} had the highest probability of failing to meet the dissolved oxygen ({DO}) standard in the Arroyo Colorado river watershed. The uncertainties arising from non-point source ({NPS}) estimation schemes were also evaluated and the Modified Rational method provided conservative estimates. Other decision support tools for evaluating various policy options were also developed. A framework for ranking model conceptualizations under stakeholder defined criteria was developed using fuzzy {TOPSIS} technique. The results indicated that inclusion of sediment oxygen demand was important for conservatism. Similarly, the impacts of future urbanization were also evaluated by developing plausible land use land cover projections in {GIS}. Characterizing urbanization was critical for establishing optimal {BOD} loadings regardless of the chosen conceptual model. An integrated probabilistic–fuzzy preference maximization model was developed and illustrated to simultaneously reconcile statistical randomness in biokinetic parameters and vague stakeholder preferences. This model provided equitable loading for each permitted compliance system. The developed decision support system tools are useful for fostering scientifically–credible, risk-informed and consensus-oriented waste load allocation schemes.},
	institution = {Texas A\&M University - Kingsville},
	type = {phdthesis},
	author = {Hernandez, Elma Annette},
	date = {2007-08},
	keywords = {{BMPs}, Fuzzy sets, Imprecision, Multi-criteria decision making, Vagueness},
	annotation = {{ISBN}: 9780549142898 Volume: Doctor of Philosophy},
}

@thesis{gutenson_rapid_2016,
	location = {Tuscaloosa},
	title = {Rapid flood damage prediction in short lead-time scenarios},
	rights = {All rights reserved},
	url = {http://ir.ua.edu/handle/123456789/2577},
	abstract = {Research on short lead-time flood inundation and damage assessment traditionally focuses on developing tools for long term planning. Studies that investigate the ability to provide short lead-time analyses are uncommon because of sparcity and spatial inconsistency of short-term hydrologic forecasts. Recent advances in continental scale hydrology make possible the ability to predict discharge at nearly all locations in the conterminous United States ({CONUS}). Theoretically, these discharge outputs enable investigation into the plausibility of short lead-time hydraulic, and damage analyses during flood events. A system predicting the hydraulics and damage potential of floods is only feasible after addressing a number of limitations. For instance, hydraulic models require a characterization of the stream channel, either through site surveys or through approximations. Further, the damage estimation methodology requires building inventories, which are developable by similar mechanisms. This dissertation advances knowledge on how to possibly address these limitations. The first study in this dissertation investigates the use of hydraulic geometries. Hydraulic geometries relate river channel geometry to bankfull discharge. However, the research presented here indicates that hydraulic geometries may estimate both the channel geometry and multiple depths of flow, under certain geomorphic and anthropogenic constraints. Accurate channel geometries are necessary for hydraulic modeling. Depth of flow estimates are useful in developing stage-discharge rating curves and possibly as a standalone means of estimating inundation grids. The second and third studies in this research look to investigate a framework for rapid flood damage assessment using public domain cadastral or parcel geospatial data. The second study discusses a fuzzy logic framework for meshing emergency response Address Points with parcel data and appropriate depth-damage relationships to determine both percent and fiscal impact of flood damage. Results of the second study highlight the effectiveness of determining flood damage with cadastral data using fuzzy text matching. The third study investigates what inputs from the cadastral data are necessary for the fuzzy logic framework to approximate a detailed flood damage investigation. Results indicate that fuzzy logic can approximate a detailed study when provided discrete use descriptions, market value, and square footage.},
	institution = {University of Alabama},
	type = {phdthesis},
	author = {Gutenson, Joseph Lee},
	date = {2016-08},
	annotation = {Volume: Doctor of Philosophy},
}

