@thesis{sundaresan_bioremediation_1997-1,
	location = {Kingsville},
	title = {Bioremediation of Arabian Light Crude Oil in Wetland Microcosms},
	url = {http://proquest.umi.com/pqdweb?did=740445871&sid=5&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {A study was conducted to evaluate the efficiency of a treatment strategy to bioremediate weathered Arabian light crude oil in wetland microcosms. A set of eighteen microcosms were constructed with wetland sediment, divided into two treatment sets of nine each, and sampled at three different sampling events–day 0, day 63, and day 106. A precise volume of artificially weathered Arabian light crude oil (corresponding to a sediment concentration of 5μg/g) was spilled in these microcosms. The efficiency of treatment-(({NH}\${\textbackslash}sb4\$)\${\textbackslash}sb3\${PO}\${\textbackslash}sb4\$ + {KNO}\${\textbackslash}sb3\$) was evaluated relative to intrinsic bioremediation. ({NH}\${\textbackslash}sb4\$)\${\textbackslash}sb3\${PO}\${\textbackslash}sb4\$ and {KNO}\${\textbackslash}sb3\$ was added at day 0, and on a weekly basis from day 49 in the treatment set. The sediment cores extruded at the different sampling events were processed for analysis using a {HP} 5980 {GC}/{HP} 5972 Mass spectrometer, and Iatrascan. Overall, the treatment series showed a higher removal of the crude oil, and higher biomass levels compared to the intrinsic series. The study also revealed the relative biodegradability of two isoprenoids–pristane, and phytane. The biomass levels in the treatment series were also a few magnitudes higher in the treatment series. Although the removal of the crude oil cannot be attributed to biodegradation alone, the greater removal of the target compounds and increased biomass levels indicated that the treatment strategy was effective, and hence worthwhile to examine in a meso-scale simulation.},
	pagetotal = {84},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Sundaresan, Anand},
	date = {1997-05},
	annotation = {{ISBN}: 9780591327854 Volume: Master of Science},
}

@thesis{mamidi_biodegradation_1994,
	location = {Kingsville},
	title = {Biodegradation of Fumonisin B1 Mycotoxin in Kingsville Corn Field Soils},
	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=743167281&sid=1&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {Biological treatment of organic compounds is widely recognized as an efficient, cost effective technique. It was identified that many organic compounds can be mineralized to non-toxic end products by both bacterial and fungal species. Several kinetic growth models accurately describe microbial growth during biological transformation. A protocol was designed during batch biodegradation experiments to determine the abilities of four different microorganisms in degrading Fumonisin B\${\textbackslash}sb1,\$ a common mycotoxin contaminant of field grown corn. During each experiment, simultaneous determination of Fumonisin removal and microbial biomass growth were made over time. Percent degradation values at the end of each sampling period were obtained by comparing the amount of Fumonisin from test systems with the values from controls. The concentration of Fumonisin was determined using High Performance Liquid Chromatography ({HPLC}) and the retention time of Fumonisin was found to be 2.6 min and that the Fumonisin is metabolized into two other compounds. The identification of these compounds is beyond the scope of the present study, and will be determined in future studies. Maximum growth and degradation was observed with microbial isolate 5 at 2 μl volume. In all cases, microbial growth and degradation of Fumonisin seemed to be correlated. An initial lag phase was observed in all microbial isolates and Fumonisin volumes higher than 2 μl.},
	pagetotal = {147},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Mamidi, Mamatha},
	date = {1994-12},
	annotation = {Volume: Master of Science},
}

@thesis{veeramachaneni_biodegradability_1992,
