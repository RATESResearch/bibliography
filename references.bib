@thesis{veerabhadraiah_predicting_1992,
	location = {Kingsville, Texas},
	title = {Predicting the Movement of Arsenic in Three South Texas Clay Soils},
	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=748195461&sid=2&Fmt=7&clientId=1449&RQT=309&VName=PQD},
	abstract = {The movement of arsenic was studied on three predominant South Texas clay soils (Willacy, Orelia and Victoria). Dynamic sorption of these soils was studied at different concentration for two inorganic arsenic species (arsenate and arsenite). Freundlich and Langmuir Isotherms were evaluated from equilibrium studies on all the three types of soil considering both arsenite and arsenate. Column studies were also conducted on the soils, to simulate the actual field conditions. Three concentrations were considered for the arsenate species. An average concentration was considered for arsenite. Victoria which has more organic, ferrous and clay content had showed the greatest sorption followed by Orelia and Willacy. Sorption of arsenate was greater than arsenite in all types of soils. Increase in sorption of both arsenate and arsenite was observed with increase in initial applied concentration. Break through curves from the column studies, indicated a steady state condition. In Column depth profile studies showed that Orelia and Willacy behaved similarly where as, Victoria had a large drop in arsenic concentration across the profile.},
	pagetotal = {128},
	institution = {Texas A\&I University},
	type = {Masters Thesis},
	author = {Veerabhadraiah, Shiva},
	date = {1992-12},
}

