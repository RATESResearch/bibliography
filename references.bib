@thesis{velasquez_design_1997-2,
	location = {Kingsville},
	title = {Design and Validation of a Mesocale Ecosystem to Simulate Insitu Intertidal Conditions of a Marsh Wetland},
	url = {http://proquest.umi.com/pqdweb?did=740445911&sid=5&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The goal of this research was to design, construct and validate a mesocosm to simulate intertidal conditions of a marsh wetland. Design included selecting the size of the mesocosm and incorporating hydrological and soil features of a wetland environment. The mesocosm was subjected to a semidiurnal tidal cycle for three months to allow for acclimation prior to testing. Nutrients such as nitrogen and phosphorus are essential elements for plant growth. Their availability determines the amount of organic matter available to support food webs. For this reason nitrate, phosphorus and total kjeldahl nitrogen were measured in the gravity outflow water, reservoir water and the soil. Biomass, a quantitative measure of the amount of living material present, was monitored in the both the gravity outflow and reservoir water as well as in the plants in the mesocosm. Since microbe biomass in water is difficult to determine, microbial biomass was correlated to total extractable adenosinetriphosphate. The precision of the P-value increased with time for all parameters analyzed across the mesocosm.},
	pagetotal = {136},
	institution = {Texas A\&M University - Kingsville},
	type = {Masters Thesis},
	author = {Velasquez, Daniel Enrique},
	date = {1997-05},
}

@thesis{veerabhadraiah_predicting_1992-2,
	location = {Kingsville, Texas},
	title = {Predicting the Movement of Arsenic in Three South Texas Clay Soils},
	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=748195461&sid=2&Fmt=7&clientId=1449&RQT=309&VName=PQD},
	abstract = {The movement of arsenic was studied on three predominant South Texas clay soils (Willacy, Orelia and Victoria). Dynamic sorption of these soils was studied at different concentration for two inorganic arsenic species (arsenate and arsenite). Freundlich and Langmuir Isotherms were evaluated from equilibrium studies on all the three types of soil considering both arsenite and arsenate. Column studies were also conducted on the soils, to simulate the actual field conditions. Three concentrations were considered for the arsenate species. An average concentration was considered for arsenite. Victoria which has more organic, ferrous and clay content had showed the greatest sorption followed by Orelia and Willacy. Sorption of arsenate was greater than arsenite in all types of soils. Increase in sorption of both arsenate and arsenite was observed with increase in initial applied concentration. Break through curves from the column studies, indicated a steady state condition. In Column depth profile studies showed that Orelia and Willacy behaved similarly where as, Victoria had a large drop in arsenic concentration across the profile.},
