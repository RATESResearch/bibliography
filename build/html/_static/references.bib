	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=743167281&sid=1&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {Biological treatment of organic compounds is widely recognized as an efficient, cost effective technique. It was identified that many organic compounds can be mineralized to non-toxic end products by both bacterial and fungal species. Several kinetic growth models accurately describe microbial growth during biological transformation. A protocol was designed during batch biodegradation experiments to determine the abilities of four different microorganisms in degrading Fumonisin B\${\textbackslash}sb1,\$ a common mycotoxin contaminant of field grown corn. During each experiment, simultaneous determination of Fumonisin removal and microbial biomass growth were made over time. Percent degradation values at the end of each sampling period were obtained by comparing the amount of Fumonisin from test systems with the values from controls. The concentration of Fumonisin was determined using High Performance Liquid Chromatography (HPLC) and the retention time of Fumonisin was found to be 2.6 min and that the Fumonisin is metabolized into two other compounds. The identification of these compounds is beyond the scope of the present study, and will be determined in future studies. Maximum growth and degradation was observed with microbial isolate 5 at 2 μl volume. In all cases, microbial growth and degradation of Fumonisin seemed to be correlated. An initial lag phase was observed in all microbial isolates and Fumonisin volumes higher than 2 μl.},
	school = {Texas A\&M University - Kingsville},
	author = {Mamidi, Mamatha},
	month = dec,
	year = {1994},
	annote = {Volume: Master of Science},
	annote = {Volume: Master of Science},
}

@mastersthesis{veeramachaneni_biodegradability_1992,
	address = {Kingsville},
	title = {Biodegradability of {Selected} {Oils} {Using} {Biosurfactant} {Producing} {Organisms}},
	url = {http://proquest.umi.com.libsrv.wku.edu/pqdweb?did=748191591&sid=2&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The purpose of this research was to determine the abilities of two different biosurfactant producing microorganisms in degrading petroleum hydrocarbons from two selected oils. To achieve this, a suitable protocol was designed during the preliminary experiments. Batch experiments were conducted in 10 ml culture tubes with biomass of each type of culture that had been enriched for the oils used. During each experiment, simultaneous determinations of petroleum hydrocarbon removal and biomass growth were made over time. Percentage degradation values at the end of each sampling period were obtained by comparing the TPH values from test systems with the values from control systems. Both the mixed cultures initiated emulsification after a 24 hr period. Maximum growth and degradation was observed with Seal Beach Mix organisms with both the oils at both concentrations. This was most likely the result of the culture diversity of this mixture. In all cases growth and degradation were correlated. An initial lag phase was observed in all sets with the higher concentrations of oils.},
