@phdthesis{bokhim_development_2003,
	address = {Kingsville},
	type = {Thesis},
	title = {Development of a two-dimensional hydrodynamic model of the tidal portion of lower {Rio} {Grande} {River}, {Texas}},
	abstract = {A two-dimensional, depth averaged, finite element hydrodynamic numerical model was designed and applied for simulation of the hydrodynamics of the tidal portion of the LRGR (Lower Rio Grande River), Texas using Research Management Associates (RMA-2) software. A part of the Gulf of Mexico was also included in the mesh because tidal water affects the flow of the river, especially near the mouth. Boundary conditions were designated both by inflow (constant at upstream boundary in the study domain) and by the water surface elevation (dynamic head) at a radial distance of one mile from the mouth. The model output at each time step specified is comprised of water surface elevation and horizontal velocity. The animation of the water velocity showing the water flow inside the mesh and the flow trace were created through the post-processing attribute of SMS. RMA-2 could be used effectively to model the hydrodynamics of the tidal portion. However due to the unavailability of additional data set, the simulated values could not be verified with data from a different source.},
	school = {Texas A\&M University - Kingsville},
	author = {Bokhim, Bijay},
	month = may,
	year = {2003},
	annote = {Volume: Master of Science},
	annote = {Volume: Master of Science},
}

