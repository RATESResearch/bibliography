@mastersthesis{velasquez_design_1997,
	address = {Kingsville},
	title = {Design and {Validation} of a {Mesocale} {Ecosystem} to {Simulate} {Insitu} {Intertidal} {Conditions} of a {Marsh} {Wetland}},
	url = {http://proquest.umi.com/pqdweb?did=740445911&sid=5&Fmt=2&clientId=1449&RQT=309&VName=PQD},
	abstract = {The goal of this research was to design, construct and validate a mesocosm to simulate intertidal conditions of a marsh wetland. Design included selecting the size of the mesocosm and incorporating hydrological and soil features of a wetland environment. The mesocosm was subjected to a semidiurnal tidal cycle for three months to allow for acclimation prior to testing. Nutrients such as nitrogen and phosphorus are essential elements for plant growth. Their availability determines the amount of organic matter available to support food webs. For this reason nitrate, phosphorus and total kjeldahl nitrogen were measured in the gravity outflow water, reservoir water and the soil. Biomass, a quantitative measure of the amount of living material present, was monitored in the both the gravity outflow and reservoir water as well as in the plants in the mesocosm. Since microbe biomass in water is difficult to determine, microbial biomass was correlated to total extractable adenosinetriphosphate. The precision of the P-value increased with time for all parameters analyzed across the mesocosm.},
	school = {Texas A\&M University - Kingsville},
	author = {Velasquez, Daniel Enrique},
	month = may,
	year = {1997},
	annote = {ISBN: 9780591327892 Volume: Master of Science},
	annote = {ISBN: 9780591327892 Volume: Master of Science},
}

@mastersthesis{veerabhadraiah_predicting_1992,
